# Variable definitions for AFT integration
variable "aft_timestamp" {
  description = "AFT execution timestamp"
  type        = string
  default     = "{{ timestamp }}"
}

variable "quota_override_value" {
  description = "Override the default quota value if needed"
  type        = number
  default     = null
}

variable "notification_email" {
  description = "Email address for quota notifications"
  type        = string
  default     = "keneaft@gmail.com"
}